<?xml version="1.0" encoding="UTF-8" ?>
<widget id="com.digitalharm.eddie" version="1.7" android-versionCode="1040000" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
	<name>Eddie Underground</name>
	<description>Collect all the coins that Eddie could carry.</description>
	<author email="support@digitalharmony.agency" href="digitalharmony.agency">Digital Harmony</author>

	

	<icon src="www/icons/icon-256.png" width="256" height="256" density="xxxhdpi" />
	<icon src="www/icons/icon-128.png" width="128" height="128" density="xhdpi" />
	<icon src="www/icons/icon-114.png" width="114" height="114" />
	<icon src="www/icons/icon-32.png" width="32" height="32" />
	<icon src="www/icons/icon-16.png" width="16" height="16" />

	<plugin name="cordova-plugin-inappbrowser" />
	<plugin name="cordova-plugin-ad-admob" />
	<plugin name="cordova-plugin-whitelist" />
	<preference name="orientation" value="landscape" />
	<preference name="fullscreen" value="false" />
	<preference name="DisallowOverscroll" value="true" />
	<preference name="LoadUrlTimeoutValue" value="120000" />
	<access origin="*" />

	<engine name="android" />

	<platform name="android">
		<splash src="www/icons/default@2x~universal~anyany.png" />		<preference name="android-minSdkVersion" value="27" />
		<preference name="android-targetSdkVersion" value="27" />
	</platform>

	<preference name="phonegap-version" value="cli-8.0.0" /></widget>
